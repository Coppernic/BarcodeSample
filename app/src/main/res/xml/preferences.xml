<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <ListPreference
        android:defaultValue="@string/pref_barcode_reader_default"
        android:dialogTitle="@string/pref_title_barcode_reader_type"
        android:entries="@array/pref_barcode_reader_entries"
        android:entryValues="@array/pref_barcode_reader_values"
        android:key="key_barcode_reader"
        android:title="@string/pref_title_barcode_reader"/>
    <fr.coppernic.sample.barcode.preferences.AutoCompletePreference
        android:key="key_barcode_port"
        android:title="@string/pref_barcode_port_title"
        />
    <ListPreference
        android:defaultValue="@string/pref_barcode_bdt_default"
        android:dialogTitle="@string/pref_barcode_bdt_title"
        android:entries="@array/pref_barcode_bdt_entries"
        android:entryValues="@array/pref_barcode_bdt_values"
        android:key="key_barcode_bdt"
        android:title="@string/pref_barcode_bdt_title"
        />
    <CheckBoxPreference
        android:defaultValue="false"
        android:key="key_use_connector"
        android:summary="@string/barcode_use_connector_summary"
        android:title="@string/barcode_use_connector"/>
</PreferenceScreen>